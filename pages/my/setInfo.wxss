.page{
    height: 100vh;
    background:rgba(237,237,237,1);
    padding: 23rpx 20rpx 0;
    box-sizing: border-box;
}
.box{
    height:363rpx;
    background:rgba(255,255,255,1);
    border-radius:14rpx;
}
.line{
    position: relative;
    border-bottom: 1rpx solid rgba(236,236,236,1);
    padding: 0 26rpx 0 33rpx;
    line-height: 120rpx;
    height: 120rpx;
}
.line:last-child{
    border-bottom: none;
}
.w-key{
    float: left;
    font-size:32rpx;
    /* font-weight:500; */
    color:rgba(128,128,128,1);
}
.v-val{
    float: right;
    width: 450rpx;
    text-align: right;
}
.v-val text{
    font-size:30rpx;
    /* font-weight:500; */
    color:rgba(0,0,0,1);
}
.adver{
    width:88rpx;
    height:88rpx;
    border-radius:50%;
    vertical-align: middle;
}
.arrow{
    width:12rpx;
    height:21rpx;
    vertical-align: middle;
    margin-left: 30rpx;
}
.box-two{
    margin-top: 20rpx;
    min-height:613rpx;
    background:rgba(255,255,255,1);
    border-radius:14rpx;
}